{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "no-empty-interface": false,
        "no-var-requires": false,
        "member-access": false,
        "max-line-length": false,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "array-type": [
            true,
            "array"
        ],
        "no-console": false,
        "trailing-comma": false,
        "jsx-no-bind": false,
        "jsx-no-lambda": false,
        "jsx-no-multiline-js": false
    },
    "rulesDirectory": [],
    "no-console": false
}